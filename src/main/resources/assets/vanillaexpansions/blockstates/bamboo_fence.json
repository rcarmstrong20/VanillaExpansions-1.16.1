{
    "multipart":
	[
        {   "apply":
			[
				{"model": "vanillaexpansions:block/bamboo_fence_post1"},
				{"model": "vanillaexpansions:block/bamboo_fence_post2"},
				{"model": "vanillaexpansions:block/bamboo_fence_post3"},
				{"model": "vanillaexpansions:block/bamboo_fence_post4"}
			]
		},
        {   "when": { "north": "true" },
            "apply": 
			[
				{ "model": "vanillaexpansions:block/bamboo_fence_side4" },
				{ "model": "vanillaexpansions:block/bamboo_fence_side3" },
				{ "model": "vanillaexpansions:block/bamboo_fence_side2" },
				{ "model": "vanillaexpansions:block/bamboo_fence_side1" }
			]
        },
        {   "when": { "east": "true" },
            "apply":
			[
				{ "model": "vanillaexpansions:block/bamboo_fence_side3", "y": 90 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side2", "y": 90 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side4", "y": 90 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side1", "y": 90 }
			]
        },
        {   "when": { "south": "true" },
            "apply":
			[
				{ "model": "vanillaexpansions:block/bamboo_fence_side3", "y": 180 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side1", "y": 180 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side4", "y": 180 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side2", "y": 180 }
			]
        },
        {   "when": { "west": "true" },
            "apply":
			[
				{ "model": "vanillaexpansions:block/bamboo_fence_side3", "y": 270 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side2", "y": 270 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side1", "y": 270 },
				{ "model": "vanillaexpansions:block/bamboo_fence_side4", "y": 270 }
			]
        }
    ]
}
