{
    "multipart":
	[
        {   "apply":
			[
				{"model": "ve:block/bamboo1_fence_post"},
				{"model": "ve:block/bamboo2_fence_post"},
				{"model": "ve:block/bamboo3_fence_post"},
				{"model": "ve:block/bamboo4_fence_post"}
			]
		},
        {   "when": { "north": "true" },
            "apply": 
			[
				{ "model": "ve:block/bamboo1_fence_side" },
				{ "model": "ve:block/bamboo2_fence_side" },
				{ "model": "ve:block/bamboo3_fence_side" },
				{ "model": "ve:block/bamboo4_fence_side" }
			]
        },
        {   "when": { "east": "true" },
            "apply":
			[
				{ "model": "ve:block/bamboo1_fence_side", "y": 90 },
				{ "model": "ve:block/bamboo2_fence_side", "y": 90 },
				{ "model": "ve:block/bamboo3_fence_side", "y": 90 },
				{ "model": "ve:block/bamboo4_fence_side", "y": 90 }
			]
        },
        {   "when": { "south": "true" },
            "apply":
			[
				{ "model": "ve:block/bamboo1_fence_side", "y": 180 },
				{ "model": "ve:block/bamboo2_fence_side", "y": 180 },
				{ "model": "ve:block/bamboo3_fence_side", "y": 180 },
				{ "model": "ve:block/bamboo4_fence_side", "y": 180 }
			]
        },
        {   "when": { "west": "true" },
            "apply":
			[
				{ "model": "ve:block/bamboo1_fence_side", "y": 270 },
				{ "model": "ve:block/bamboo2_fence_side", "y": 270 },
				{ "model": "ve:block/bamboo3_fence_side", "y": 270 },
				{ "model": "ve:block/bamboo4_fence_side", "y": 270 }
			]
        }
    ]
}
